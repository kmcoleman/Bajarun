{
  "expo": {
    "name": "Nor Cal Moto ADV",
    "slug": "baja-run-2026",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bajarun2026",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a5f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ncmotoadv.bajarun",
      "buildNumber": "9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Nor Cal Moto ADV uses your camera to take a profile photo and to capture photos and videos during tours to share with fellow riders in the group gallery.",
        "NSPhotoLibraryUsageDescription": "Nor Cal Moto ADV accesses your photo library so you can select a profile photo and choose photos or videos to share with fellow riders in the group gallery.",
        "NSPhotoLibraryAddUsageDescription": "Nor Cal Moto ADV saves tour photos and videos from the group gallery to your device so you can keep your favorite memories.",
        "NSMicrophoneUsageDescription": "Nor Cal Moto ADV uses your microphone to record audio when capturing videos to share with fellow riders in the group gallery."
      }
    },
    "android": {
      "package": "com.ncmotoadv.bajarun",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1e3a5f"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "bajarun2026"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#1e3a5f"
        }
      ],
      "expo-apple-authentication",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.73531449020-55bab060gne2c4iqf3u5m77rvde2h6qh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4547c6e6-463a-471d-b9e8-dab16d069dfb"
      }
    }
  }
}
